<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".ResultActivity">

    <android.support.constraint.Guideline
            android:orientation="horizontal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" app:layout_constraintGuide_begin="182dp" android:id="@+id/guideline6"
            app:layout_constraintGuide_percent="0.3"/>
    <android.support.constraint.Guideline
            android:orientation="horizontal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" app:layout_constraintGuide_begin="289dp" android:id="@+id/guideline7"
            app:layout_constraintGuide_percent="0.5"/>
    <android.support.constraint.Guideline
            android:orientation="vertical"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" app:layout_constraintGuide_begin="139dp" android:id="@+id/guideline8"
            app:layout_constraintGuide_percent="0.08"/>
    <android.support.constraint.Guideline
            android:orientation="vertical"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" app:layout_constraintGuide_begin="295dp" android:id="@+id/guideline9"
            app:layout_constraintGuide_percent="0.92"/>
    <view android:layout_width="314dp" android:layout_height="165dp"
          class="android.support.v7.widget.AppCompatTextView"
          id="@+id/view" android:id="@+id/ResultLabel"
          android:textSize="60sp" android:gravity="center" android:maxLines="3"
          app:autoSizeMaxTextSize="60sp" app:autoSizeStepGranularity="1sp" app:autoSizeTextType="uniform"
          android:layout_marginStart="8dp"
          app:layout_constraintStart_toStartOf="@+id/guideline8" android:layout_marginEnd="8dp"
          app:layout_constraintEnd_toStartOf="@+id/guideline9" android:layout_marginTop="8dp"
          app:layout_constraintTop_toTopOf="parent" android:layout_marginBottom="8dp"
          app:layout_constraintBottom_toTopOf="@+id/imageView3" android:text="홍길동님의\n로또번호입니다"/>
    <ImageView
            android:layout_width="44dp"
            android:layout_height="105dp" app:srcCompat="@drawable/ball_01"
            android:id="@+id/imageView3"
            app:layout_constraintStart_toStartOf="@+id/guideline8" app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintEnd_toStartOf="@+id/imageView4" android:layout_marginStart="8dp"
            android:layout_marginTop="8dp" app:layout_constraintTop_toTopOf="@+id/guideline6"
            android:layout_marginBottom="8dp" app:layout_constraintBottom_toTopOf="@+id/guideline7"/>
    <ImageView
            android:layout_width="44dp"
            android:layout_height="105dp" app:srcCompat="@drawable/ball_03"
            android:id="@+id/imageView4"
            app:layout_constraintStart_toEndOf="@+id/imageView3" app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintEnd_toStartOf="@+id/imageView5" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toTopOf="@+id/guideline7" android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="@+id/guideline6"/>
    <ImageView
            android:layout_width="45dp"
            android:layout_height="105dp" app:srcCompat="@drawable/ball_10"
            android:id="@+id/imageView5"
            app:layout_constraintStart_toEndOf="@+id/imageView4" app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintEnd_toStartOf="@+id/imageView6" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toTopOf="@+id/guideline7" android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="@+id/guideline6"/>
    <ImageView
            android:layout_width="44dp"
            android:layout_height="105dp" app:srcCompat="@drawable/ball_11"
            android:id="@+id/imageView6"
            app:layout_constraintStart_toEndOf="@+id/imageView5"
            app:layout_constraintHorizontal_bias="0.5" app:layout_constraintEnd_toStartOf="@+id/imageView7"
            android:layout_marginBottom="8dp" app:layout_constraintBottom_toTopOf="@+id/guideline7"
            android:layout_marginTop="8dp" app:layout_constraintTop_toTopOf="@+id/guideline6"/>
    <ImageView
            android:layout_width="44dp"
            android:layout_height="105dp" app:srcCompat="@drawable/ball_21"
            android:id="@+id/imageView7"
            app:layout_constraintStart_toEndOf="@+id/imageView6" app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintEnd_toStartOf="@+id/imageView8" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toTopOf="@+id/guideline7" android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="@+id/guideline6"/>
    <ImageView
            android:layout_width="44dp"
            android:layout_height="105dp" app:srcCompat="@drawable/ball_32"
            android:id="@+id/imageView8"
            app:layout_constraintStart_toEndOf="@+id/imageView7" app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintEnd_toStartOf="@+id/guideline9" android:layout_marginEnd="8dp"
            android:layout_marginBottom="8dp" app:layout_constraintBottom_toTopOf="@+id/guideline7"
            android:layout_marginTop="8dp" app:layout_constraintTop_toTopOf="@+id/guideline6"/>
    <ImageView
            android:layout_width="0dp"
            android:layout_height="0dp" app:srcCompat="@drawable/money"
            android:id="@+id/imageView9"
            app:layout_constraintEnd_toStartOf="@+id/guideline9" android:layout_marginEnd="8dp"
            app:layout_constraintStart_toStartOf="@+id/guideline8" android:layout_marginStart="8dp"
            android:layout_marginTop="8dp" app:layout_constraintTop_toTopOf="@+id/guideline7"
            android:layout_marginBottom="8dp" app:layout_constraintBottom_toBottomOf="parent"/>
</android.support.constraint.ConstraintLayout>